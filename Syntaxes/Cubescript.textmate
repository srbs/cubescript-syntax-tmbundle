		keyword = {
			patterns = (
				{	name = 'keyword.control.cubescript';
					match = '(\?)|(\b((case(f|s)?)|(loop(files|list|while)?)|(clear)?sleep|cond|if|listfind|result|while)\b)';
				},
				{	name = 'keyword.operator.comparison.cubescript';
