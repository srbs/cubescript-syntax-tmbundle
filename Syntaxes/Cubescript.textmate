				},
				{	name = 'keyword.operator.assignment.cubescript';
					match = '(=)';
				},
				{	name = 'keyword.operator.arithmetic.cubescript';
					match = '((\-|\+|\*)f?)|(\b(div|mod)f?\b)';
				},
				{	name = 'keyword.operator.logical.cubescript';
					match = '(!|&&|\|\|)';
				},
				{	name = 'keyword.operator.bitwise.cubescript';
					match = '(\^|\^~|&|&~|<<|>>|\||\|~|~)';
				},
				{	name = 'punctuation.terminator.cubescript';
					match = ';';
				},
			);
		};
		
		
		block = {
			patterns = (
				{	include = '#block_bracket'; },
				{	include = '#block_paren'; },
			);
		};
		
		block_bracket = {
			patterns = (
				{	name = 'meta.block.bracket.cubescript';
					begin = '\[';
					end = '\]';
					patterns = ( { include = '#code'; } );
				},
			);
		};
		
		block_paren = {
			patterns = (
				{	name = 'meta.block.paren.cubescript';
					begin = '\(';
					end = '\)';
					patterns = ( { include = '#code'; } );
				},
			);
		};
		
		
		invalid = {
			patterns = (
				{	name = 'invalid.illegal.character.cubescript';
					match = '[\)\]]';
				},
			);
		};
	};
}
