// name:          Cubescript
// keyEquivalent: ^~C (Ctrl+Option+Shift+C)

// rules:
// - alias names cannot contain (, ), [, ], /, ;, or whitespace
// - function names cannot contain ", $, @, (, ), [, ], /, ;, or whitespace

{	scopeName = 'source.cubescript';
	fileTypes = ( 'cfg' );
	foldingStartMarker = '\[[^\[\]]*(//.*)?$';
	foldingStopMarker = '^[^\[]*\].*';
	
	patterns = ( { include = '#code'; } );
	
	repository = {
		code = {
			patterns = (
				{	include = '#comment'; },
				{	include = '#values'; },
				{	include = '#advanced_support'; },
				{	include = '#support'; },
				{	include = '#keyword'; },
				{	include = '#block'; },
				{	include = '#invalid'; },
				// interesting idea of marking non-quoted strings,
				// but it would capture non-default commands as well.
				//{	name = 'meta.string.other.cubescript';
				//	match = '[^\s\(\)\[\];]+';
				//},
			);
		};
		
		
		
		comment = {
			patterns = (
				{	name = 'comment.line.double-slash.cubescript';
					begin = '//';
					end = '$\n?';
					captures = { 0 = { name = 'punctuation.definition.comment.cubescript'; }; };
				},
			);
		};
		
		
		values = {
			patterns = (
				{	include = '#string'; },
				{	include = '#number'; },
				{	include = '#variable'; },
			);
		};
		
		string = {
			patterns = (
				{	name = 'string.quoted.double.cubescript';
					begin = '"';
					end = '"';
					beginCaptures = { 0 = { name = 'punctuation.definition.string.begin.cubescript'; }; };
					endCaptures = { 0 = { name = 'punctuation.definition.string.end.cubescript'; }; };
					patterns = (
						// escape characters
						{	name = 'constant.character.escape.cubescript';
							match = '\^([nt"\^]|f[0-7sr])';
						},
						{	name = 'invalid.illegal.escape.unknown.cubescript';
							match = '\^(.|\n|\r)';
						},
						
						// placeholder characters
						// ?? only within (format) call.. but how??
						{	name = 'constant.other.placeholder.cubescript';
							match = '%[1-9%]';
						},
						// ?? only within (format) call.. but how??
						//{	name = 'invalid.illegal.placeholder.unknown.cubescript';
						//	match = '%(.|\n|\r)';
						//},
						
						// illegal characters
						{	name = 'invalid.illegal.string.newline.cubescript';
							match = '[\n\r]';
						},
					);
				},
			);
		};
		
		number = {
			patterns = (
				{	name = 'constant.numeric.hex.cubescript';
					match = '\b0[xX][\h]+\b(?!\.)';
				},
				{	name = 'constant.numeric.octal.cubescript';
					match = '\b0[0-7]+\b';
				},
				{	name = 'constant.numeric.integer.cubescript';
					match = '\b[0-9]+\b(?!\.)';
				},
				{	name = 'constant.numeric.hex-float.cubescript';
					match = '(?x)
						(?<!\w)                    # Ensure word boundary
						0[xX]                      # Start literal
						(
							\.[\h]+                # Allow starting .
							| [\h]+(\.[\h]*)?      # Allow leading digits & optional suffix
						)
						([pP][+-]?[0-9]+)?         # Exponent
						(?!\w)                     # Ensure word boundry
					';
				},
				{	name = 'constant.numeric.float.cubescript';
					match = '(?x)
						(?<!\w)                    # Ensure word boundary
						(
							\.[0-9]+               # Allow starting .
							| [0-9]+(\.[0-9]*)?    # Allow leading digits & optional suffix
						)
						([eE][+-]?[0-9]+)?         # Exponent
						(?!\w)                     # Ensure word boundry
					';
				},
			);
		};
		
		// some day support built-in function callbacks? (see 'language feature variables below')
		// multiplayer:   intermission, mapstart, demostart, demoend, gamehud, edithud, allowedittoggle
		// single player: can_teleport_[0-9]+, level_trigger_[0-9]+, monster_dead_[0-9]+
		variable = {
			patterns = (
				{	name = 'meta.definition.variable.cubescript';
					// variable dereferencing must occur after
					// 1) BOL; 2) whitespace; 3) opening/closing of ()/[]; 4) after a string ends;
					begin = '(^|(?<=[ \t\n\(\)\[\]"]))(\$|\@+)';
					// variable dereferencing ends before
					// 1) EOL; 2) comment; 3) whitespace; 4) semicolon; 5) closing of []/();
					end = '($|(?=//|[\s;\)\]]))';
					beginCaptures = { 2 = { name = 'variable.dereference.cubescript'; }; };
					patterns = (
						// language feature variables
						// ?? TODO mark as 'support.variable.cubescript'
						// ?? TODO move into #support_vars
						{	name = 'variable.language.cubescript';
							match = '(arg[1-9][0-9]*|numargs|guirollovername|guirolloveraction|guirolloverimgpath|guirolloverimgaction|commandbuf)\b';
						},
						// support built in variables
						{	include = '#support_vars'; },
						// support `@(func arg1 arg2)`
						{	name = 'meta.evaluation.eager.cubescript';
							begin = '(?<=@)(?=\()';
							end = '(?<=\))';
							patterns = ( { include = '#block_paren'; } );
						},
						// this would allow single '/' within var names, but since I can't
						// get the same logic working elsewhere, it will stay commented out
						//{	name = 'variable.other.cubescript';
						//	match = '/($|[^/])';
						//},
						// variable names (follow rules above)
						{	name = 'variable.other.cubescript';
							match = '[^\s\(\)\[\]/;]*';
						},
						// all others are illegal
						{	name = 'invalid.illegal.name.cubescript';
							match = '[^\s;]+';
						},
					);
				},
			);
		};
		
		
		advanced_support = {
			patterns = (
				{	include = '#binds'; },
				{	include = '#guis'; },
				{	include = '#functions'; },
			);
		};
		
		binds = {
			patterns = (
				{	name = 'meta.definition.bind.cubescript';
					begin = '(?x)
						\b((get)?(bind|editbind|specbind))
						\s+
						(?!["\$@\[\(]) # dont validate when using "str"/$var/@var/[text]/(func)
					';
					end = '\s+';
					beginCaptures = { 1 = { name = 'support.function.cubescript'; }; };
					patterns = (
						{	include = '#comment'; },
						{	name = 'support.constant.key.cubescript';
							match = '(?i)\b([A-Z0-9]|AMPERSAND|ASTERISK|AT|BACKQUOTE|BACKSLASH|BACKSPACE|BREAK|CAPSLOCK|CARET|CLEAR|COLON|COMMA|COMPOSE|DELETE|DOLLAR|DOWN|END|EQUALS|ESCAPE|EXCLAIM|F1|F10|F11|F12|F13|F14|F15|F2|F3|F4|F5|F6|F7|F8|F9|GREATER|HASH|HELP|HOME|INSERT|KP0|KP1|KP2|KP3|KP4|KP5|KP6|KP7|KP8|KP9|KP_DIVIDE|KP_ENTER|KP_EQUALS|KP_MINUS|KP_MULTIPLY|KP_PERIOD|KP_PLUS|LALT|LCTRL|LEFT|LEFTBRACKET|LEFTPAREN|LESS|LMETA|LSHIFT|LSUPER|MENU|MINUS|MODE|MOUSE1|MOUSE2|MOUSE3|MOUSE4|MOUSE5|MOUSE6|MOUSE7|MOUSE8|NUMLOCK|PAGEDOWN|PAGEUP|PAUSE|PERIOD|PLUS|PRINT|QUESTION|QUOTE|QUOTEDBL|RALT|RCTRL|RETURN|RIGHT|RIGHTBRACKET|RIGHTPAREN|RMETA|RSHIFT|RSUPER|SCROLLOCK|SEMICOLON|SLASH|SPACE|SYSREQ|TAB|UNDERSCORE|UP)\b';
						},
						{	name = 'invalid.illegal.constant.key.cubescript';
							match = '[^\s\(\)\[\];]+';
						},
					);
				},
			);
		};
		
		guis = {
			patterns = (
				{	name = 'meta.definition.gui.cubescript';
					begin = '\b(newgui)\s+([^\s\$\@\(\)\[\]/;]*)\s+(?=\[)';
					end = '(?<=\])';
					beginCaptures = {
						1 = { name = 'support.function.cubescript'; };
						2 = { name = 'entity.name.gui.cubescript'; };
					};
					patterns = ( { include = '#block_bracket'; } );
				},
			);
		};
		
		functions = {
			patterns = (
				{	name = 'meta.definition.function.alias.cubescript';
					// support `alias <name> [...]`
					begin = '(?x)
						^
						(alias)\s+
						([^\s"\$\@\(\)\[\]/;]+)
						\s+(?=\[)
					';
					end = '(?<=\])';
					beginCaptures = {
						1 = { name = 'keyword.operator.assignment.cubescript'; };
						2 = { name = 'entity.name.function.cubescript'; };
					};
					patterns = ( { include = '#block_bracket'; } );
				},
				{	name = 'meta.definition.function.equals.cubescript';
					// support `<name> = [...]`
					begin = '(?x)
						^
						([^\s"\$\@\(\)\[\]/;]+)
						\s+(=)\s+
						(?=\[)
					';
					end = '(?<=\])';
					beginCaptures = {
						1 = { name = 'entity.name.function.cubescript'; };
						2 = { name = 'keyword.operator.assignment.cubescript'; };
					};
					patterns = ( { include = '#block_bracket'; } );
				},
			);
		};
		
		
		support = {
			patterns = (
				{	include = '#support_funcs'; },
				{	include = '#support_vars'; },
			);
		};
		
